/* QStackedWidget中包含的控件样式 */
QStackedWidget QLabel {
    color: #404040;
    font-size: 12px;
    background-color: transparent;
}

QStackedWidget QPushButton {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 12px;
    min-height: 24px;
}

QStackedWidget QPushButton:hover {
    background-color: #f9f9f9;
    border-color: #d5d5d5;
}

QStackedWidget QPushButton:pressed {
    background-color: #f0f0f0;
}

QStackedWidget QPushButton[objectName*=&quot;delete&quot;], QStackedWidget QPushButton[objectName*=&quot;remove&quot;] {
    color: #e95c63;
}

QStackedWidget QPushButton[objectName*=&quot;delete&quot;]:hover, QStackedWidget QPushButton[objectName*=&quot;remove&quot;]:hover {
    background-color: #fff8f8;
    color: #d64550;
}

QStackedWidget QSlider::handle {
    background-color: #6495ed;
    border-radius: 7px;
    width: 14px;
    height: 14px;
    margin: -4px 0;
}

QStackedWidget QSlider::groove:horizontal {
    height: 6px;
    background-color: #e2e2e2;
    border-radius: 3px;
}

QStackedWidget QLineEdit {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 8px;
}

QStackedWidget QCheckBox {
    spacing: 5px;
}

QStackedWidget QCheckBox::indicator {
    border: 1px solid #d0d0d0;
    background-color: #ffffff;
    border-radius: 3px;
    width: 16px;
    height: 16px;
}

QStackedWidget QCheckBox::indicator:checked {
    background-color: #6495ed;
    border-color: #6495ed;
}

QStackedWidget QComboBox {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 8px;
    min-height: 24px;
}

QStackedWidget QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 20px;
    border-left: none;
}

QStackedWidget QComboBox::down-arrow {
    width: 8px;
    height: 8px;
    border-top: 5px solid #a0a0a0;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
}

QStackedWidget QScrollBar:vertical, QStackedWidget QScrollBar:horizontal {
    background: #f5f5f7;
    width: 8px;
    height: 8px;
}

QStackedWidget QScrollBar::handle:vertical, QStackedWidget QScrollBar::handle:horizontal {
    background: #d0d0d0;
    border-radius: 4px;
}

QStackedWidget QScrollBar::add-line, QStackedWidget QScrollBar::sub-line {
    height: 0px;
    width: 0px;
}</string>
