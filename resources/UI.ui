}


/* 按钮样式 */
QPushButton {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 12px;
    min-height: 24px;
}

QPushButton:hover {
    background-color: #f9f9f9;
    border-color: #d5d5d5;
}

QPushButton:pressed {
    background-color: #f0f0f0;
}

/* 删除按钮样式 */
QPushButton#deleteButton, QPushButton[objectName*=&quot;delete&quot;], QPushButton[objectName*=&quot;remove&quot;] {
    color: #e95c63;
}

QPushButton#deleteButton:hover, QPushButton[objectName*=&quot;delete&quot;]:hover, QPushButton[objectName*=&quot;remove&quot;]:hover {
    background-color: #fff8f8;
    color: #d64550;
}

/* 文本框样式 */
QTextEdit, QLineEdit {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 8px;
}

/* 下拉框样式 */
QComboBox {
    background-color: #ffffff;
    color: #404040;
    border: 1px solid #e2e2e2;
    border-radius: 6px;
    padding: 4px 8px;
    min-height: 24px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 20px;
    border-left: none;
}

QComboBox::down-arrow {
    width: 8px;
    height: 8px;
    border-top: 5px solid #a0a0a0;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
}

/* 菜单样式 */
QMenuBar {
    background-color: #f5f5f7;
    border-bottom: 1px solid #e8e8e8;
}

QMenu {
    background-color: #ffffff;
    border: 1px solid #e8e8e8;
    border-radius: 6px;
}

/* 滚动条样式 */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #f5f5f7;
    width: 8px;
    height: 8px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #d0d0d0;
    border-radius: 4px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    height: 0px;
    width: 0px;
}

/* 标签页样式 */
QTabWidget::pane {
    border: 1px solid #e2e2e2;
    background-color: #ffffff;
    border-radius: 6px;
}

QTabBar::tab {
    background-color: #f0f0f2;
